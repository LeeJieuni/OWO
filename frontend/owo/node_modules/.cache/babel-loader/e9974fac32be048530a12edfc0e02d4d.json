>\n    </div>\n  </div>\n</template>\n\n<script>\n// import { useStore } from 'vuex';\n// import { reactive, computed } from 'vue';\nimport Carousel from '@/components/MainPage/Carousel.vue';\nimport rankingList from '@/components/MainPage/rankingList.vue';\nimport roomTab from '@/components/MainPage/roomTab.vue';\nimport swal from 'sweetalert2';\nimport { mapGetters } from 'vuex';\n\nwindow.Swal = swal;\n\nconst accounts = 'accounts';\n\nexport default {\n  components: {\n    Carousel,\n    rankingList,\n    roomTab,\n  },\n  computed: {\n    ...mapGetters(accounts, ['userInfo', 'isLogin']),\n  },\n  setup() {\n    const onInputImage = () => {\n      const input = document.querySelector('.input-image');\n      console.log(input.files[0].type);\n    };\n    return {\n      onInputImage,\n    };\n  },\n  methods: {\n    setCookie() {\n      this.$cookies.set('nosee', 'Y', '7d');\n    },\n  },\n  created() {\n    const check = this.$cookies.get('nosee');\n    if (check !== null) { // 쿠키가 있는 경우\n      if (this.userInfo.weight === null && this.isLogin === true && check !== 'Y') {\n        this.$swal({\n          title: '#오운완',\n          input: 'checkbox',\n          inputPlaceholder: '일주일 간 보지 않기',\n          html:\n            '추가 정보를 입력하시면, <br>더 많은 서비스를 이용하실 수 있습니다. <br> <a href=\"mypage/update\"><strong>My Page</strong></a>로 이동하기. <hr>',\n          icon: 'info',\n          showCloseButton: true,\n        }).then((res) => {\n          if (res.value === 1) {\n            this.setCookie();\n          }\n        });\n      }\n    }\n    if (check === null) { // 쿠키가 없는 경우\n      if (this.userInfo.weight !== null && this.isLogin === true) {\n        this.$swal({\n          title: '#오운완',\n          input: 'checkbox',\n          inputPlaceholder: '일주일 간 보지 않기',\n          html:\n            '추가 정보를 입력하시면, <br>더 많은 서비스를 이용하실 수 있습니다. <br> <a href=\"mypage/update\"><strong>My Page</strong></a>로 이동하기. <hr>',\n          icon: 'info',\n          showCloseButton: true,\n        }).then((res) => {\n          if (res.value === 1) {\n            this.setCookie();\n          }\n        });\n      }\n    }\n  },\n};\n</script>\n\n<style scoped>\n.start-btn {\n    margin-right: 30px;\n}\n\n.btn-lg {\n  height: 40px;\n  text-align: center;\n  line-height: 10px;\n  font-size: 18px;\n}\n.owo {\n  font-weight: 600;\n  text-shadow: #7b7b7b 1px 1px;\n}\n\n.first-intro-text {\n  font-size: 18px;\n  line-height: 15px;\n}\n.btns {\n  padding-top: 5px;\n  padding-bottom: 20px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}