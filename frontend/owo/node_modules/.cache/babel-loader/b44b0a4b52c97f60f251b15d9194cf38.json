}\n\n      e.preventDefault();\n      store.dispatch('accounts/addGoals', inputgoals);\n    }; // const updateGoal = () => {\n    //   for (let i = 0; i < this.goals.length; i += 1) {\n    //     if (this.goals[i] === this.inputGoalExercise) {\n    //     }\n    //   }\n    // }\n\n\n    const tagModal = () => {\n      /* eslint-disable */\n      // alert('해당 주간 목표를 삭제하시겠습니까?');\n      // Swal.fire({\n      //   icon: 'question',\n      //   title: '주간 목표',\n      //   text: '변경하시겠어요?',\n      //   footer: '<a href=\"\">Why do I have this issue?</a>'\n      // })\n      e.preventDefault();\n      console.log(e);\n    }; // console.log('이미지시작')\n    // console.log(typeof(profileImg));\n    // console.log('이미지끝')\n    // Methods\n    // const onInputImage = () => {\n    //   const input = document.querySelector('.input-image');\n    //   console.log(input.files);\n    // };\n\n\n    return {\n      state,\n      user,\n      addGoal,\n      goals,\n      profileImg\n    };\n  },\n\n  components: {},\n\n  created() {\n    // console.log(this.user.id);\n    console.log(this.goals);\n  }\n\n};","map":{"version":3,"mappings":"AA+EA,SAASA,QAAT,QAAyB,MAAzB;AACA,SAASC,QAAT,EAAmBC,QAAnB,QAAmC,KAAnC,C,CACA;;AAEA,eAAe;EACbC,IAAI,EAAE,kBADO;;EAEbC,IAAI,GAAG;IACL,OAAO,CACL;IADK,CAAP;EAGD,CANY;;EAObC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,EAAtB,CADM,CACoB;IAC1B;;IACA,MAAMO,IAAG,GAAIL,QAAQ,CAAC,MAAMI,KAAK,CAACE,OAAN,CAAc,mBAAd,CAAP,CAArB;IACA,MAAMC,KAAI,GAAIP,QAAQ,CAAC,MAAMI,KAAK,CAACE,OAAN,CAAc,gBAAd,CAAP,CAAtB;IACA,MAAME,UAAS,GAAIR,QAAQ,CAAC,MAAMI,KAAK,CAACE,OAAN,CAAc,qBAAd,CAAP,CAA3B,CALM,CAON;IACA;IACA;IACA;IACA;;IACA,MAAMG,KAAI,GAAIV,QAAQ,CAAC;MACrBW,QAAQ,EAAE,EADW;MAErBC,OAAO,EAAE,CACP;QAAEC,IAAI,EAAE,IAAR;QAAcC,KAAK,EAAE;MAArB,CADO,EAEP;QAAED,IAAI,EAAE,IAAR;QAAcC,KAAK,EAAE;MAArB,CAFO,EAGP;QAAED,IAAI,EAAE,IAAR;QAAcC,KAAK,EAAE;MAArB,CAHO,EAIP;QAAED,IAAI,EAAE,IAAR;QAAcC,KAAK,EAAE;MAArB,CAJO,EAKP;QAAED,IAAI,EAAE,OAAR;QAAiBC,KAAK,EAAE;MAAxB,CALO,CAFY;MASrBC,KAAK,EAAE;QACLC,KAAK,EAA