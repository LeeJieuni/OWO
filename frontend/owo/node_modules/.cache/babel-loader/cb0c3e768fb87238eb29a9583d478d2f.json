{"ast":null,"code":"import { defineComponent, reactive, computed } from 'vue';\nimport { Carousel, Slide } from 'vue3-carousel';\nimport { useStore } from 'vuex';\nimport 'vue3-carousel/dist/carousel.css';\nexport default defineComponent({\n  name: 'AutoPlay',\n  components: {\n    Carousel,\n    Slide\n  },\n\n  setup() {\n    const store = useStore();\n    const isLogin = computed(() => store.getters['accounts/isLogin']);\n    const state = reactive({\n      selected: '',\n      options: [{\n        text: '없음',\n        value: '1'\n      }, {\n        text: '약간',\n        value: '2'\n      }, {\n        text: '중간',\n        value: '3'\n      }, {\n        text: '많음',\n        value: '4'\n      }, {\n        text: '매우 많음',\n        value: '5'\n      }]\n    });\n    return {\n      state,\n      isLogin\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAqCA,SAASA,eAAT,EAA0BC,QAA1B,EAAoCC,QAApC,QAAoD,KAApD;AACA,SAASC,QAAT,EAAmBC,KAAnB,QAAgC,eAAhC;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEA,OAAO,iCAAP;AAEA,eAAeL,eAAe,CAAC;EAC7BM,IAAI,EAAE,UADuB;EAE7BC,UAAU,EAAE;IACVJ,QADU;IAEVC;EAFU,CAFiB;;EAM7BI,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,EAAtB;IACA,MAAMK,OAAM,GAAIR,QAAQ,CAAC,MAAMO,KAAK,CAACE,OAAN,CAAc,kBAAd,CAAP,CAAxB;IACA,MAAMC,KAAI,GAAIX,QAAQ,CAAC;MACrBY,QAAQ,EAAE,EADW;MAErBC,OAAO,EAAE,CACP;QAAEC,IAAI,EAAE,IAAR;QAAcC,KAAK,EAAE;MAArB,CADO,EAEP;QAAED,IAAI,EAAE,IAAR;QAAcC,KAAK,EAAE;MAArB,CAFO,EAGP;QAAED,IAAI,EAAE,IAAR;QAAcC,KAAK,EAAE;MAArB,CAHO,EAIP;QAAED,IAAI,EAAE,IAAR;QAAcC,KAAK,EAAE;MAArB,CAJO,EAKP;QAAED,IAAI,EAAE,OAAR;QAAiBC,KAAK,EAAE;MAAxB,CALO;IAFY,CAAD,CAAtB;IAUA,OAAO;MACLJ,KADK;MAELF;IAFK,CAAP;EAID;;AAvB4B,CAAD,CAA9B","names":["defineComponent","reactive","computed","Carousel","Slide","useStore","name","components","setup","store","isLogin","getters","state","selected","options","text","value"],"sourceRoot":"","sources":["C:\\Users\\multicampus\\Desktop\\새 폴더2\\S07P12C202\\frontend\\owo\\src\\components\\MainPage\\Carousel.vue"],"sourcesContent":["<template>\n<div class=\"container-fluid\">\n  <div class=\"title\">\n    <h3 class=\"lg-title\" style=\"text-align: left; margin-left: 5px;\">\n      실제<span class=\"owo-text\">#오운완</span>을 사용하시는 회원님입니다.\n    </h3>\n  </div>\n  <div class=\"carousel\">\n    <Carousel :autoplay=\"2000\" :items-to-show=\"3\" :wrap-around=\"true\">\n      <Slide v-for=\"slide in 10\" :key=\"slide\">\n        <div class=\"carousel__item\">{{ slide }}</div>\n      </Slide>\n      <template #addons>\n      </template>\n    </Carousel>\n  </div>\n    <div class=\"content\">\n        <h1><span class=\"owo\"># 오운완</span>과 함께 시작해볼까요?</h1>\n        <p class=\"first-intro-text\"><span class=\"owo\"># 오운완</span>과 함께 하신 회원님들입니다!</p>\n        <p class=\"first-intro-text\">여러분들도 재미있게 운동을 즐기실 수 있습니다!</p>\n        <!-- <div class=\"btns\"> -->\n        <router-link v-if=\"!isLogin\" to=\"/login\">\n          <b-button pill variant=\"outline-secondary\" class=\"btn-lg start-btn\">\n            Start with us!\n          </b-button>\n        </router-link>\n        <a v-else to=\"\">\n          <b-button pill variant=\"outline-secondary\" class=\"btn-lg start-btn\">\n            Start with us!\n          </b-button>\n        </a>\n        <!-- </div> -->\n      </div>\n</div>\n</template>\n\n<script>\nimport { defineComponent, reactive, computed } from 'vue';\nimport { Carousel, Slide } from 'vue3-carousel';\nimport { useStore } from 'vuex';\n\nimport 'vue3-carousel/dist/carousel.css';\n\nexport default defineComponent({\n  name: 'AutoPlay',\n  components: {\n    Carousel,\n    Slide,\n  },\n  setup() {\n    const store = useStore();\n    const isLogin = computed(() => store.getters['accounts/isLogin']);\n    const state = reactive({\n      selected: '',\n      options: [\n        { text: '없음', value: '1' },\n        { text: '약간', value: '2' },\n        { text: '중간', value: '3' },\n        { text: '많음', value: '4' },\n        { text: '매우 많음', value: '5' },\n      ],\n    });\n    return {\n      state,\n      isLogin,\n    };\n  },\n});\n</script>\n\n<style scope>\n.owo-text {\n  /* color: #4E8AFF; */\n  font-size: 32px;\n}\n.owo {\n  /* color: #4E8AFF; */\n  font-weight: 600;\n  text-shadow: #7b7b7b 1px 1px;\n}\n.container-fluid {\n  padding-top: 30px;\n  height: 100%;\n}\n.carousel {\n  /* padding-top: 10px; */\n  padding-bottom: 10px;\n}\n.carousel__item {\n  min-height: 380px;\n  width: 100%;\n  background-color: var(--vc-clr-primary);\n  color:  var(--vc-clr-white);\n  font-size: 20px;\n  border-radius: 8px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.lg-title {\n  font-weight: 800;\n  font-size: 28px;\n}\n.start-btn {\n  border-radius: 30%;\n  margin-bottom: 30px;\n}\n\n.carousel__slide {\n  padding: 10px;\n}\n\n.carousel__prev,\n.carousel__next {\n  box-sizing: content-box;\n  /* border: 5px solid white; */\n}\n.first-intro-text {\n  font-size: 18px;\n  line-height: 15px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}