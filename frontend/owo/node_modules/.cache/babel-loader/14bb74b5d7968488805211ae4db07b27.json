{"ast":null,"code":"import { createNamespacedHelpers } from 'vuex';\nconst {\n  mapActions,\n  mapGetters\n} = createNamespacedHelpers('accounts');\nexport default {\n  data() {\n    return {\n      passwordErr: false,\n      credentials: {\n        email: '',\n        password: ''\n      },\n      confirm: ''\n    };\n  },\n\n  computed: { ...mapGetters(['signupInfo'])\n  },\n  methods: { ...mapActions({\n      register: 'register'\n    }),\n\n    moveToLogin() {\n      this.$router.push('/login');\n    },\n\n    isSame() {\n      const password1 = this.credentials.password;\n      const password2 = this.confirm;\n\n      if (password1 !== password2) {\n        this.passwordErr = true;\n      } else {\n        this.passwordErr = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8DA,SAASA,uBAAT,QAAwC,MAAxC;AAEA,MAAM;EAAEC,UAAF;EAAcC;AAAd,IAA6BF,uBAAuB,CAAC,UAAD,CAA1D;AAEA,eAAe;EACbG,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,KADR;MAELC,WAAW,EAAE;QACXC,KAAK,EAAE,EADI;QAEXC,QAAQ,EAAE;MAFC,CAFR;MAMLC,OAAO,EAAE;IANJ,CAAP;EAQD,CAVY;;EAWbC,QAAQ,EAAE,EACR,GAAGP,UAAU,CAAC,CAAC,YAAD,CAAD;EADL,CAXG;EAcbQ,OAAO,EAAE,EACP,GAAGT,UAAU,CAAC;MACZU,QAAQ,EAAE;IADE,CAAD,CADN;;IAIPC,WAAW,GAAG;MACZ,KAAKC,OAAL,CAAaC,IAAb,CAAkB,QAAlB;IACD,CANM;;IAOPC,MAAM,GAAG;MACP,MAAMC,SAAQ,GAAI,KAAKX,WAAL,CAAiBE,QAAnC;MACA,MAAMU,SAAQ,GAAI,KAAKT,OAAvB;;MACA,IAAIQ,SAAQ,KAAMC,SAAlB,EAA6B;QAC3B,KAAKb,WAAL,GAAmB,IAAnB;MACF,CAFA,MAEO;QACL,KAAKA,WAAL,GAAmB,KAAnB;MACF;IACD;;EAfM;AAdI,CAAf","names":["createNamespacedHelpers","mapActions","mapGetters","data","passwordErr","credentials","email","password","confirm","computed","methods","register","moveToLogin","$router","push","isSame","password1","password2"],"sourceRoot":"","sources":["C:\\Users\\multicampus\\Desktop\\새 폴더2\\S07P12C202\\frontend\\owo\\src\\views\\accounts\\registerView.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"register\">\n      <div class=\"container\">\n      <form @submit.prevent=\"register(credentials)\"\n      class=\"registerForm\">\n      <h2 class=\"mb-3\">회원가입</h2>\n      <div class=\"input\">\n        <label for=\"email\">아이디<input\n          v-model=\"credentials.email\"\n          class=\"form-control\"\n          type=\"email\"\n          name=\"email\"\n          placeholder=\"email@adress.com\"\n          required\n        /></label>\n      </div>\n      <div class=\"input\">\n        <label for=\"password\"><span>비밀번호</span><input\n          v-model=\"credentials.password\"\n          class=\"form-control\"\n          type=\"password\"\n          name=\"password\"\n          placeholder=\"password123\"\n          required\n          @change=\"isSame()\"\n        /></label>\n      </div>\n      <div class=\"input\">\n        <label for=\"confirm\"><span>비밀번호 확인</span><input\n          v-model=\"confirm\"\n          class=\"form-control\"\n          type=\"password\"\n          placeholder=\"password123\"\n          @change=\"isSame()\"\n          required\n        /></label>\n      </div>\n      <small v-if=\"passwordErr\">비밀번호가 일치하지 않습니다.</small>\n      <div class=\"alternative-option mt-4\">\n       오운완 회원이신가요?\n       <b-button Style=\"font-size: 15px\" variant=\"link\" @click=\"moveToLogin\">로그인</b-button>\n      </div>\n      <div class=\"socialregister\" style=\"padding: 10px\">\n        <div class=\"socialbtn d-flex justify-content-around\">\n          <span>구글</span>\n          <span>네이버</span>\n          <span>카카오</span>\n        </div>\n      </div>\n      <b-button type=\"submit\"\n      class=\"mt-3 btn-pers\" id=\"register_button\" variant=\"outline-primary\">\n        회원가입\n      </b-button>\n    </form>\n    </div>\n  </div>\n  </div>\n</template>\n\n<script>\n\nimport { createNamespacedHelpers } from 'vuex';\n\nconst { mapActions, mapGetters } = createNamespacedHelpers('accounts');\n\nexport default {\n  data() {\n    return {\n      passwordErr: false,\n      credentials: {\n        email: '',\n        password: '',\n      },\n      confirm: '',\n    };\n  },\n  computed: {\n    ...mapGetters(['signupInfo']),\n  },\n  methods: {\n    ...mapActions({\n      register: 'register',\n    }),\n    moveToLogin() {\n      this.$router.push('/login');\n    },\n    isSame() {\n      const password1 = this.credentials.password;\n      const password2 = this.confirm;\n      if (password1 !== password2) {\n        this.passwordErr = true;\n      } else {\n        this.passwordErr = false;\n      }\n    },\n  },\n};\n</script>\n\n<style>\n.container {\n  display: flex;\n  height: 100vh;\n  justify-content: center;\n  align-items: center;\n}\n.registerForm {\n  padding: 50px;\n  border: solid 1px rgb(177, 177, 181);\n  border-radius: 10%;\n}\nlabel {\n  flex: 1;\n  text-align: left;\n  margin-bottom: 10px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}